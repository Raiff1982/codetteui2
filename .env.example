# Codette Environment Configuration
# Copy this file to .env and fill in your values

# IMPORTANT: Never commit .env files with real values to version control
# This file contains only examples and placeholders

# Frontend Configuration
VITE_API_BASE_URL=http://localhost:8000    # Base URL for backend API
VITE_API_URL=${VITE_API_BASE_URL}         # Legacy API URL support
VITE_ENV=development                       # Environment: development/production
VITE_DEBUG=true                           # Enable debug logging and features
VITE_DEPLOYMENT_TARGET=local              # Deployment: local/staging/production
PROD=false                                # Production mode flag

# Error Tracking
VITE_SENTRY_DSN=your_sentry_dsn_here     # Sentry.io DSN for error tracking

# Supabase Integration
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Backend Configuration (for production deployment)
API_HOST=0.0.0.0
API_PORT=8000
ENVIRONMENT=development

# Database Configuration
DATABASE_URL=sqlite:///backend/data/codette.db

# Security Configuration
SECRET_KEY=your_secret_key_for_jwt_tokens_here

# CORS Configuration (comma-separated)
CORS_ORIGINS=http://localhost:5173,https://codette.online

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_BURST=10

# AI System Configuration
AI_SYSTEMS_ENABLED=true
QUANTUM_MAX_DIMENSION=50
NEURAL_PREDICTION_LIMIT=5

# Logging
LOG_LEVEL=INFO
LOG_FILE=backend/logs/codette.log

# External Services (Optional)
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Development vs Production
# For development: use localhost URLs
# For production: use your actual domain URLs
# Example production:
# VITE_API_BASE_URL=https://api.your-domain.com
# CORS_ORIGINS=https://your-domain.com,https://www.your-domain.com

# Production Settings
# Set ENVIRONMENT=production for production deployment
# Use strong SECRET_KEY in production
# Configure proper CORS_ORIGINS for your domain